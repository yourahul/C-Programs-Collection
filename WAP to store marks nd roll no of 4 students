#include<stdio.h>
int main(){
	int rows=4;
	int colums=2;
	int arr[4][2];
	
	printf("enter the mrks and roll no of 4 students \n");
	
	for(int i=0; i<4;i++){
		printf("student %d \n",i+1);
		printf("roll no ");
		scanf("%d",&arr[i][0]);
		printf("marks ");
		scanf("%d",&arr[i][1]);
	}
	
	printf("\n");
	for(int i=0;i<4;i++){
		printf("%d %d \n",arr[i][0],arr[i][1]);
	}
	
	return 0;
}
