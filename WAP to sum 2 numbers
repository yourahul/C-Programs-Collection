#include <stdio.h>

int add(int a,int b){
    return a+b;
}

int main() {
  int a,b;
  printf("enter first number");
  scanf("%d",&a);
  printf("enter second number");
  scanf("%d",&b);
  
  int sum = add(a,b);   //here add is not a Lib function.hence we have to take it as int add>>
  
  printf("%d",sum);
  
   return 0;
}
